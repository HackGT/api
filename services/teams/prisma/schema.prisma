generator client {
  provider = "prisma-client-js"
  output   = "./generated"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URI_TEAMS_SERVICE")
}

enum CommitmentLevel {
  LOW
  MEDIUM
  HIGH
}

model User {
  userId          String          @id @unique
  name            String
  email           String          @unique
  matched         Boolean         @default(false)
  school          String
  year            String
  major           String
  description     String
  skills          String[]
  commitmentLevel CommitmentLevel
}
