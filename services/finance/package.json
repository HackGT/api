{
  "name": "@api/services-finance",
  "version": "1.0.0",
  "main": "dist/app",
  "types": "dist/app",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn prisma-generate",
    "build": "tsc --build --force",
    "clean": "tsc --build --clean",
    "dev": "yarn prisma-generate && nodemon --watch 'src/**/*' --exec \"ts-node\" -r tsconfig-paths/register src/app.ts",
    "migrate:dev": "dotenv -e ../../config/.env prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma-generate": "prisma format && prisma generate",
    "seed": "prisma db seed --preview-feature",
    "start": "yarn migrate:deploy && yarn start:normal",
    "start:normal": "node -r tsconfig-paths/register dist/app.js"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0"
  },
  "dependencies": {
    "@api/common": "*",
    "@api/config": "*",
    "@casl/ability": "^5.4.4",
    "@prisma/client": "^4.16.2",
    "@sentry/node": "^7.16.0",
    "@sentry/tracing": "^7.16.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.13",
    "@types/request-promise": "^4.1.48",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "firebase-admin": "^10.0.2",
    "fs": "^0.0.1-security",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "prisma": "^4.16.2",
    "request": "^2.88.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.8.3"
  }
}
