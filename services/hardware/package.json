{
  "name": "@api/services-hardware",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn prisma-generate",
    "build": "tsc --build --force",
    "clean": "tsc --build --clean",
    "dev": "nodemon --watch 'src/**/*' --exec \"ts-node\" -r tsconfig-paths/register src/app.ts",
    "migrate:dev": "dotenv -e ../../config/.env prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma-generate": "prisma format && prisma generate",
    "seed": "prisma db seed",
    "start": "yarn migrate:deploy && yarn start:normal",
    "start:normal": "node -r tsconfig-paths/register dist/app.js"
  },
  "dependencies": {
    "@api/common": "1.0.0",
    "@api/config": "1.0.0",
    "@casl/ability": "^5.4.4",
    "@prisma/client": "^5.0.0",
    "@sentry/node": "^7.16.0",
    "@sentry/tracing": "^7.16.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.16",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "helmet": "^6.0.0",
    "morgan": "^1.10.0",
    "prisma": "^5.0.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1"
  }
}
